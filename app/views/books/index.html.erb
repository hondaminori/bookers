<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <div class="field">
    <label for="book_title">Title</label>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <label for="book_body">Body</label>
    <%= f.text_area :body %>
  </div>

  <%= f.submit 'create book' %>
<% end %>

